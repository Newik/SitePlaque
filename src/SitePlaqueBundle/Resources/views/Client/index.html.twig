{% block body %}
    <h1>Liste des clients</h1>

    <table>
        <thead>
        <th>Nom</th>
        <th>Prénom</th>
        <th>Entreprise</th>
        <th>Siret</th>
        <th>Téléphone</th>
        </thead>
        {% for client in arrayClients %} <!-- for qui va lister les colonnes du tableau -->
        <body>
        <tr id="row-{{ client.Id }}">
            <td>{{ client.nomClient }}</td>
            <td>{{ client.prenomClient }}</td>
            <td >{{ client.raisonSociale }}</td>
            <td>{{ client.siret }}</td>
            <td>{{ client.tel }}</td>
        </tr>
        {% else %}
            <tr><td>Aucun client n'a été trouvé.</td></tr>
        {% endfor %}
        </body>
    </table>

{% endblock %}